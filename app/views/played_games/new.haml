- title "New Played Game"
- content_for :footer do
  = javascript_include_tag 'played_games'
%h1 New Played Game

= simple_form_for @played_game, html: {novalidate: true} do |f|
  %fieldset
    %ol
      = f.input :game_name, input_html:{ data: {autocomplete_source: autocomplete_games_url(subdomain:'www')}}
      %li
        %button{ type: 'submit' } Create Game

= render 'shared/user_account_links'
