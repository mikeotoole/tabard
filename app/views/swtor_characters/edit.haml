- title "Editing Star Wars: The Old Republic Character"

#body
  %h1 Editing Star Wars: The Old Republic Character

  = simple_form_for @swtor_character, :html => {:novalidate => true}  do |f|
    %fieldset
      %ol
        = render 'fields', :f => f, :swtor_character => @swtor_character
        %li
          %button{ :type => 'submit' } Save character
